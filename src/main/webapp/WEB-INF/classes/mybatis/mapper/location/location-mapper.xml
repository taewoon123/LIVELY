<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="location">
    <select id="getLocationList" resultType="LocationVo">
        SELECT
        LOCATION_NO,
        LOCATION_NAME
        FROM LOCATION
    </select>
</mapper>

